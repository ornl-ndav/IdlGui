#$Id$

# Makefile for BSSreduction
# Compile the necessary modules
# Create the VM programs 
# Copy the VM programs in their final location

all:
	idl ./makefile.pro;
install:
	cp bss_reduction_sqe.sav /SNS/software/idltools/sav/
	cp BSSreduction_sqe /SNS/software/idltools/
	cp images/*.bmp /SNS/software/idltools/sav/images/

